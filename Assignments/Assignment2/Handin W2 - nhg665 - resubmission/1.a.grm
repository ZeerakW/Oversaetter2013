%token <string> aChar bChar
%token EOF

%start S

%type <string list> S T B

%%

S : T EOF           { $1 }

T : aChar T         { print "Rule aT\n";[$1] @ $2 }
  | aChar B         { print "Rule aA\n";[$1] @ $2 }
;

B : aChar B bChar   { print "Rule aAb\n";[$1] @ $2 @ [$3] }
  |                 { print "epsilon\n";[] }
;

